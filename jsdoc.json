{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ".",
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs|verovio-toolkit.js)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "readme": "README.md"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
